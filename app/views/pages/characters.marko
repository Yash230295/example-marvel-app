<layout-use("../layout.marko", {
  path: data.path
})>
  <layout-put into="content">
    <div class="page-header">
      <h1>Characters</h1>
    </div>
    <characters-list({
      characters: data.characters
    })/>
    <marvel-attribution({
      attributionText: data.attributionText
    })/>
  </layout-put>
</layout-use>
